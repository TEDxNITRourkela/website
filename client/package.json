{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "dotenv": "^8.2.0",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-countdown": "^2.3.1",
    "react-dom": "^17.0.1",
    "react-messenger-customer-chat": "^0.8.0",
    "react-router-dom": "^5.2.0",
